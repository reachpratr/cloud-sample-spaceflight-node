@Library('piper-lib-os') _
node() {
    stage('prepare') {
        checkout scm
        setupCommonPipelineEnvironment script:this
    }


stage('build') {
       sh 'mtaBuild --build-target CF --mtar cloud-sample-spaceflight-node.mtar build'
}
stage('deploy') {
        cloudFoundryDeploy script:this
}
}
